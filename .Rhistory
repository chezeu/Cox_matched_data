Matrix_function_sum<-function(n,m,C,beta){
Q<-matrix(0,n,m)
#Lvec<-matrix(0,n,m)
for (i in 1:n) {
if(i!=1 & i!=n){
pro = rep(0,m)
pro[i] = C[1] #Probability of True links
pro[i+1]= C[3]
pro[i-1]=C[3]
Q[i,]<-pro
#Lvec[i,] <- rmultinom(1, size = 1, pro = pro)
}
if(i==1)  {
pro = rep(0,m)
pro[i] =  C[1] #Probability of True links
pro[i+1]= C[2]
pro[i+2]= C[3]
Q[i,]<-pro
#Lvec[i,] <- rmultinom(1, size = 1, pro = pro)
}
if(i==n){
pro = rep(0,m)
pro[i] = C[1] #Probability of True links
pro[i-1]=C[2]
pro[i-2]=C[3]
Q[i,]<-pro
# Lvec[i,] <- rmultinom(1, size = 1, pro= pro)
}
}
return(Q=Q)
}
